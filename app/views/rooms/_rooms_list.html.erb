<% rooms.each do |room| %>
  <div class="col-md-4">
    <div class="panel panel-default">
      <div class="panel-heading preview">
        <%= image_tag room.cover_photo(:medium) %>
      </div>
      <div class="panel-body">
        <%= link_to room.listing_name, room %><br/>
        $<%= room.price %> - <%= room.home_type %>
      </div>
    </div>
  </div>
<% end %>
